{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react-native"
  ],
  "extends": [
    "airbnb",
    "plugin:react-native/all"
  ],
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "react/require-default-props": 0,
    "react/jsx-filename-extension": 0
  }
}
